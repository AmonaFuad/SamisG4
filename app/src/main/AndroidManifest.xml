<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.samisg4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.SocialActivity" android:label="@string/category_Social"
            android:parentActivityName=".activities.MainActivity"></activity>
        <activity
            android:name=".activities.ScienceActivity"
            android:label="@string/category_Science"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.MathActivity"
            android:label="@string/category_Math"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.EnglishActivity"
            android:label="@string/category_English"
            android:parentActivityName=".activities.MainActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>